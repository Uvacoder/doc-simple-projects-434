.icon .bot {
    background-color: #00374E;
    height: 3.6px;
    width: 18px;
    position: absolute;
    left: 1px;
    -webkit-transition: all .3s ease;
    transition: all .3s ease;
    -webkit-border-radius: 7.2px;
    border-radius: 7.2px
}

.icon .top {
    top: 1px
