.icon {
    position: absolute;
    cursor: pointer;
    width: 20px;
    height: 20px;
    top: 0;
    right: 20px;
    bottom: 0;
    margin: auto;
    -webkit-transition: all .3s ease;
    transition: all .3s ease;
    -webkit-border-radius: 2.4px;
    border-radius: 2.4px
}

