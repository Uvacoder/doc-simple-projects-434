.icon .mid {
    top: 8.2px
}

.icon .bot {
    top: 15.4px
}

.icon.active {
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg)
}

.icon.active .top {
    top: 8.2px;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg)
}

.icon.active .mid {
    opacity: 0;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg)
}

.icon.active .bot {
    top: 8.2px;
    -webkit-transform: rotate(-45deg);
