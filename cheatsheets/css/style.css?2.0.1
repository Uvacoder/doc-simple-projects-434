.sidebar {
    z-index: 1000;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    padding: 0 20px;
    width: 200px;
    -webkit-transition: -webkit-transform 0.2s ease;
    transition: -webkit-transform 0.2s ease;
    transition: transform 0.2s ease;
    transition: transform 0.2s ease, -webkit-transform 0.2s ease
}

.sidebar__title {
    margin-left: 12px;
    display: inline-block;
    position: relative
}

.sidebar__title:before,
.sidebar__title:after {
    position: absolute;
    top: -0.07em
}

.sidebar__title:before {
    left: -0.7em;
    content: "{"
}

.sidebar__title:after {
    right: -0.7em;
    content: "}"
}

@media (max-width: 767px) {
    .sidebar__title {
        display: none
    }
}

.sidebar__nav {
    overflow: auto;
    position: absolute;
    bottom: 70px;
    top: 50px;
    left: 20px;
    right: 20px
}

@media (min-width: 768px) {
    .sidebar__nav {
        bottom: 90px;
        top: 80px
    }
}

.sidebar__nav ul {
    margin: 0
}

.sidebar__nav li {
    list-style: none
}

.sidebar__nav a {
    position: relative
}

.sidebar__nav a:before {
    visibility: hidden;
    position: absolute;
    bottom: -2px;
    left: 0;
    width: 0%;
    height: 1px;
    background-color: #00374E;
    content: "";
    -webkit-transition: all 0.2s ease-in-out 0s;
    transition: all 0.2s ease-in-out 0s
}

.sidebar__nav a:hover:before {
    visibility: visible;
    width: 100%
}

